<%- include("header") -%>


    <div class="box" id="heading">
        <h1><%= listType %></h1>
    </div>

    <div class="box">
            <% for(var i = 0; i<=newObjective.length; i++) { %>
                <div class="item">
                    <input type="checkbox">
                    <p><%= newObjective[i] %></p>
                </div>
                    <% } %>
                
               
                    <form class="item" action="/" method="post">
                        <input type="text" name="newitem" placeholder="New task" autocomplete="off">
                        <button type="submit" name="btn" value=<%= listType %>>+</button>
                    </form>

                
    </div>
    
<%- include("footer") -%>